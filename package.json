{
    "name": "vscode-ui5",
    "displayName": "UI5 Extension for Visual Studio Code",
    "description": "SAP UI5 Extension for Visual Studio Code",
    "version": "0.3.0",
    "preview": true,
    "publisher": "milannankov",
    "engines": {
        "vscode": "^1.9.0"
    },
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "ui5",
        "snippets",
        "XML",
        "Java Script"
    ],
    "license": "SEE LICENSE IN LICENSE",
    "bugs": {
        "url": "https://github.com/milannankov/vscode-ui5/issues",
        "email": "public@nankov.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/milannankov/vscode-ui5.git"
    },
    "homepage": "https://github.com/milannankov/vscode-ui5/blob/master/README.md",
    "activationEvents": [],
    "main": "./out/extension",
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/javascript.json"
            },
            {
                "language": "xml",
                "path": "./snippets/xml.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}