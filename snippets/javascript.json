{
    "UI5 - Controller": {
        "prefix": "ui5controller",
        "body": [
            "sap.ui.define([",
            "   \"sap/ui/core/mvc/Controller\",",
            "   \"sap/ui/model/json/JSONModel\",",
            "], function (Controller, JSONModel) {",
            "   \"use strict\";",
            "   return Controller.extend(\"${1:namespace}.controller.${2:Name}\", {",
            "",
            "       onInit: function () {",
            "           var oModel = new JSONModel({",
            "               items: []",
            "           });",
            "           this.getView().setModel(oModel);",
            "       },",
            "",
            "       onBeforeRendering: function () {",
            "       },",
            "",
            "       onAfterRendering: function () {",
            "       },",
            "",
            "       onExit: function () {",
            "       },",
            "",
            "   });",
            "",
            "});"
        ],
        "description": "UI5 View Controller"
    },

    "UI5 - Advanced Custom Control": {
        "prefix": "ui5cca",
        "body": [
            "sap.ui.define([",
            "   \"sap/ui/core/Control\"",
            "], function (Control) {",
            "   \"use strict\";",
            "   return Control.extend(\"${1:namespace}.${2:customControl}\", {",
            "       metadata: {",
            "           properties: {",
            "               \"height\": { type: \"int\", default: 100 }",
            "           },",
            "           aggregations: {",
            "               \"header\": { type: \"sap.ui.core.Control\", multiple: false }",
            "           },",
            "           events: {",
            "               \"rendered\": {}",
            "           }",
            "       },",
            "",
            "       init: function () {",
            "       },",
            "",
            "       renderer: function (oRM, oControl) {",
            "",
            "           oRM.write(\"<div class=\\\"${2:customControl}\\\" \");",
            "           oRM.writeControlData(oControl);",
            "           oRM.write(\">\");",
            "           oRM.renderControl(oControl.getAggregation(\"header\"));",
            "           oRM.write(\"<div>Custom control - \" + oControl.getHeight() + \"</div>\");",
            "           oRM.write(\"</div>\");",
            "",
            "           oControl.fireEvent(\"rendered\", {});",
            "       },",
            "",
            "       onAfterRendering: function () {",
            "       },",
            "",
            "       onBeforeRendering: function () {",
            "       },",
            "",
            "       exit: function () {",
            "       }",
            "   });",
            "});"
        ],
        "description": "Custom control with aggregation, event, and properties"
    },

