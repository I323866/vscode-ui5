			"               </subHeader>",
			"               <content>",
		    "               </content>",
			"               <footer>",
			"                   <Bar>",
			"                       <contentMiddle>",
			"                       </contentMiddle>",
            "                       <contentRight>",
			"                       </contentRight>",
